<div id={{networkName}}>
<section class="section content">
	<h4 class="title is-4 has-text-primary">Summary of the analysis results <span><a href="{{zipPath}}" target="_blank">(Result Download)</a></span></h4>
<ul>
{{#summary}}
<li>{{.}}</li>
{{/summary}}
</ul>
</section>
<section class="section">
<div class="columns is-multiline">
<div class="column is-5-widescreen is-6-desktop is-12-tablet is-12-mobile">
<h4 class="title is-4 has-text-primary">Sub-network graph </h4>
	<nta-gene-network
		container-id="{{netName}}"
		:data="geneNetwork"
		:method="method"
		highlight-option="{{highlightOption}}"
		:enrichment="enrichment"
	>
	</nta-gene-network>
</div> <!-- column -->

<div class="column is-5-widescreen is-offset-2-widescreen is-6-desktop is-12-tablet is-12-mobile">
<h4 class="title is-4 has-text-primary">Enriched GO terms graph</h4>
	<nta-dag
		container-id="{{dagName}}"
		:data="dag"
	>
	</nta-dag>
</div> <!-- column -->
</div> <!-- columns -->
</section>

<section class="section">
<h4 class="title is-4 has-text-primary">Detailed Information</h4>
<div class="columns is-multiline">
<div class="column is-6-widescreen is-12-desktop is-12-tablet is-12-mobile" id="col-go">
<nta-enrichment-table :data="enrichment" :method="method" :sig-method="sigMethod" :threshold="threshold"> </nta-enrichment-table>
</div> <!-- column -->

<div class="column is-3-widescreen is-6-desktop is-6-tablet is-12-mobile" id="col-seed">
<nta-seed-table :data="seeds"> </nta-seed-table>
</div> <!-- column -->
<div class="column is-3-widescreen is-6-desktop is-6-tablet is-12-mobile" id="col-rank">
<nta-candidate-table :data="candidates" :method="method"> </nta-candidate-table>
</div> <!-- column -->

</div> <!-- columns -->
</section>
</div>
