<h4 class="title is-4 has-text-primary">Enrichment Results</h4>
<section id="enrich-result">
	<p class="select-redundancy">Redundancy reduction:
		<b-radio v-model="redundancy" native-value="all">All</b-radio>
		{{#hasAp}}
		<b-radio v-model="redundancy" native-value="ap">Affinity propagation</b-radio>
		{{/hasAp}}
		{{#hasWsc}}
		<b-radio v-model="redundancy" native-value="wsc">Set coverage</b-radio>
		{{/hasWsc}}
	</p>
	<b-tabs v-model="activeTab" type="is-toggle">
		<b-tab-item label="Bar chart">
		<div class="vertical-center-container">
			<wg-bar-plot
				:dimensions="bar.dimensions"
				:data="barData"
				:value-key="bar.valueKey"
				{{#hasGeneSetDes}}
				name-key="description"
				{{/hasGeneSetDes}}
				{{^hasGeneSetDes}}
				name-key="geneSet"
				{{/hasGeneSetDes}}
				:x-label="bar.xLabel">
			</wg-bar-plot>
		</div>
		</b-tab-item>
		<b-tab-item label="Volcano plot">
		<div class="vertical-center-container">
			<wg-volcano-plot
				:data="volcanoData"
				:x="volcano.x"
				:y="volcano.y"
				:x-label="volcano.xLabel"
				:label-types="volcano.labelTypes"
				:x-transform="volcano.xTransform"
				:radius="volcano.radius"
				:color="volcano.color"
				:tooltip-content="volcano.tooltipContent"
				:show-all-labels="false"
				:auto-place-labels="true"
			></wg-volcano-plot>
		</div>
		</b-tab-item>
		{{#hasGeneSetDag}}
		<b-tab-item label="DAG">
			<div style="width: 100%; height: 600px">
			<wg-dag
				container-id="wg_dag"
				:data="dagData"
			></wg-dag>
			</div>
		</b-tab-item>
		{{/hasGeneSetDag}}
	</b-tabs>
	<wg-gene-table
		:data="geneTables"
		:info="enrichment"
		:standard-id="standardId"
		:method="enrichMethod"
		{{#methodIsOra}}
		:mapped-input-size="mappedInputSize"
		{{/methodIsOra}}
	></wg-gene-table>
</section>
